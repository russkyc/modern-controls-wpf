<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  Primary  -->
    <SolidColorBrush x:Key="primary-lighten-3" Color="#e57373" />
    <SolidColorBrush x:Key="primary-lighten-2" Color="#ef5350" />
    <SolidColorBrush x:Key="primary-lighten-1" Color="#f44336" />
    <SolidColorBrush x:Key="primary-default" Color="#e53935" />
    <SolidColorBrush x:Key="primary-darken-1" Color="#d32f2f" />
    <SolidColorBrush x:Key="primary-darken-2" Color="#c62828" />
    <SolidColorBrush x:Key="primary-darken-3" Color="#b71c1c" />

    <!--  Status  -->
    <SolidColorBrush x:Key="success" Color="#4CAF50" />
    <SolidColorBrush x:Key="warning" Color="#FFC107" />
    <SolidColorBrush x:Key="danger" Color="#F44336" />

    <!--  Inverted  -->
    <SolidColorBrush x:Key="inverted-lighten-3" Color="#FDFBFF" />
    <SolidColorBrush x:Key="inverted-lighten-2" Color="#FFF1EF" />
    <SolidColorBrush x:Key="inverted-lighten-1" Color="#E3D3D5" />
    <SolidColorBrush x:Key="inverted-default" Color="#E3CBCE" />
    <SolidColorBrush x:Key="inverted-darken-1" Color="#E3C7C4" />
    <SolidColorBrush x:Key="inverted-darken-2" Color="#E3BDC0" />
    <SolidColorBrush x:Key="inverted-darken-3" Color="#E3B5C3" />

</ResourceDictionary>