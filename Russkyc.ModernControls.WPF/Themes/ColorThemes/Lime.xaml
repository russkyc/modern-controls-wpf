<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Primary  -->
    <SolidColorBrush x:Key="primary-lighten-3" Color="#DAEBB3" />
    <SolidColorBrush x:Key="primary-lighten-2" Color="#BFDE79" />
    <SolidColorBrush x:Key="primary-lighten-1" Color="#A4D23D" />
    <SolidColorBrush x:Key="primary-default" Color="#8AC800" />
    <SolidColorBrush x:Key="primary-darken-1" Color="#81BD00" />
    <SolidColorBrush x:Key="primary-darken-2" Color="#78B200" />
    <SolidColorBrush x:Key="primary-darken-3" Color="#6EA500" />

    <!--  Status  -->
    <SolidColorBrush x:Key="success" Color="#4CAF50" />
    <SolidColorBrush x:Key="warning" Color="#FFC107" />
    <SolidColorBrush x:Key="danger" Color="#F44336" />

    <!--  Inverted  -->
    <SolidColorBrush x:Key="inverted-lighten-3" Color="#F7FBF9" />
    <SolidColorBrush x:Key="inverted-lighten-2" Color="#EBF6E9" />
    <SolidColorBrush x:Key="inverted-lighten-1" Color="#CDE8C3" />
    <SolidColorBrush x:Key="inverted-default" Color="#B9E0AE" />
    <SolidColorBrush x:Key="inverted-darken-1" Color="#A1D69A" />
    <SolidColorBrush x:Key="inverted-darken-2" Color="#8FCF8D" />
    <SolidColorBrush x:Key="inverted-darken-3" Color="#7DC881" />

</ResourceDictionary>